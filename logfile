stage('save log build') {
    steps {
        script {
            def logContent = Jenkins.getInstance()
                .getItemByFullName(env.test1)
                .getBuildByNumber(
                    Integer.parseInt(env.1))
                .logFile.text
            // copy the log in the job's own workspace
            writeFile file: "buildlog.txt", text: logContent
        }
    }
}
